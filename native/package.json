{
  "name": "react-native-ui-components-native",
  "version": "0.0.1",
  "description": "UI Component Library",
  "private": true,
  "main": "src/index.js",
  "scripts": {
    "eslint:fix": "./node_modules/eslint/bin/eslint.js --fix --config ./.eslintrc.json --ext .js ./src/*",
    "eslint": "./node_modules/eslint/bin/eslint.js --config ./.eslintrc.json --ext .js ./src/*",
    "test": "jest",
    "watch": "jest --watch",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "keywords": [
    "react",
    "react-native",
    "components"
  ],
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.37",
    "@storybook/addon-actions": "^3.3.9",
    "@storybook/addon-links": "^3.3.9",
    "@storybook/addons": "^3.3.9",
    "@storybook/react": "^3.3.9",
    "babel-core": "^6.26.0",
    "babel-jest": "21.2.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react-native": "4.0.0",
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-16": "^1.0.2",
    "enzyme-to-json": "^3.1.2",
    "jest": "21.2.1",
    "jest-junit": "^1.5.1",
    "react-dom": "^16.2.0",
    "react-native": "0.50.2",
    "react-test-renderer": "16.0.0"
  },
  "dependencies": {
    "@storybook/addon-options": "^3.3.10",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-native-web": "^0.3.1"
  },
  "jest": {
    "preset": "react-native",
    "testResultsProcessor": "./node_modules/jest-junit",
    "setupFiles": [
      "./jest.mock.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "jest-junit": {
    "suiteName": "ui-components-native",
    "output": "./TEST-REACT-NATIVE.xml",
    "classNameTemplate": "{classname}-{title}",
    "titleTemplate": "{classname}-{title}",
    "usePathForSuiteName": "true"
  }
}
