{
  "name": "ui-components",
  "version": "0.0.1",
  "description": "A set of reusable UI components",
  "devDependencies": {
    "bower": "^1.7.7",
    "http-server": "^0.8.5",
    "jasmine-core": "^2.4.1",
    "karma": "^0.13.19",
    "karma-chrome-launcher": "^0.2.2",
    "karma-jasmine": "^0.3.7",
    "karma-junit-reporter": "^0.3.8",
    "protractor": "^3.0.0"
  },
  "scripts": {
    "postinstall": "bower install",
    "prestart": "npm install",
    "start": "http-server -a localhost -p 8000 -c-1",
    "test": "karma start karma.conf.js",
    "test-single-run": "karma start karma.conf.js  --single-run",
    "preupdate-webdriver": "npm install",
    "update-webdriver": "webdriver-manager update",
    "preprotractor": "npm run update-webdriver",
    "protractor": "protractor e2e-tests/protractor.conf.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/clouway/ui-components.git"
  },
  "keywords": [
    "ui",
    "components",
    "angular",
    "angularjs"
  ],
  "author": "clouway",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/clouway/ui-components/issues"
  },
  "homepage": "https://github.com/clouway/ui-components#readme"
}
