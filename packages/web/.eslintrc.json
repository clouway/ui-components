{
  "parser": "babel-eslint",
  "parserOptions": {
    "sourceType": "module"
  },
  "env": {
    "browser": true,
    "es6": true,
    "jest": true
  },
  "plugins": [
    "prettier"
  ],
  "rules": {
    "import/default": "off",
    "no-empty-function": "off",
    "prettier/prettier": ["error", {
      "singleQuote": true,
      "semi": false
    }]
  },
  "extends": [
    "prettier",
    "prettier/react",
    "prettier/standard"
  ]
}
